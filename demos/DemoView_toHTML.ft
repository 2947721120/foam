<%
  var test = this.data;
  var cView = DAOListView.create({
      mode: 'read-only',
      dao: test.tests.dao,
      rowView: DemoView.xbind({ mode: 'read-only' })
  });

  this.addChild(cView);
%>
<h3>{{test.name}}</h3>
<% if ( test.description ) { %>
   <div class="description">
     {{test.description}}
   </div>
   <br>
<% } %>
<div>Code:</div>
<pre><div class="code">{{test.code}}</div></pre>
$$results
<blockquote><%= cView %></blockquote>
<br>
