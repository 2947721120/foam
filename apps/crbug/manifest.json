{
  "name": "QuickBug",
  "version": "1.0",
  "manifest_version": 2,

  "permissions": [
    "<all_urls>",
    "http://*/",
    "https://*/",
    "identity",
    "storage",
    "unlimitedStorage",
    "webview"
  ],

  "icons": {
    "64": "images/logo.png"
  },

  "app": {
    "background": {
      "scripts": [
        "foam.js",
        "ImportedModels.js",
        "Models.js",
        "QIssuePreviewView.js",
        "QIssueCommentView.js",
        "QIssueCommentAuthorView.js",
        "QIssueCommentUpdateView.js",
        "QIssuePreviewView.js",
        "QIssueTileView.js",
        "DAO.js",
        "View.js",
        "QBug.js",
        "QProject.js",
        "Browser.js",
        "PreviewView.js",
        "QueryParser.js",
        "bg.js"
      ]
    }
  },

  "url_handlers": {
    "google_code_issues": {
      "matches": [
        "https://code.google.com/p/*/issues"
      ],
      "title": "Google Code Issues"
    }
  },

   "key": "",

  "oauth2": {
    "client_id": "18229540903-c1cku6fscof2takj43gedatapd7c33m9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.google.com/m8/feeds",
      "https://www.googleapis.com/auth/projecthosting"
    ]
  }
}
